{
  "name": "docodile",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build:prod": "docusaurus build --config docusaurus.config.js",
    "build:beta": "docusaurus build --config docusaurus-beta.config.js",
    "swizzle": "docusaurus swizzle",
    "rebuild-search-index:prod": "docker run -it --env-file=.env.local -e \"CONFIG=$(cat docSearchConfig.json | jq -r tostring)\" algolia/docsearch-scraper",
    "rebuild-search-index:beta": "docker run -it --env-file=.env.local -e \"CONFIG=$(cat docSearchConfig-beta.json | jq -r tostring)\" algolia/docsearch-scraper",
    "clear": "docusaurus clear",
    "serve": "node server.js",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "test": "jest",
    "redirect-404s": "ts-node scripts/getRedirectsFrom404s"
  },
  "dependencies": {
    "@algolia/client-search": "^5.6.1",
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/plugin-google-gtag": "^3.5.2",
    "@docusaurus/plugin-google-tag-manager": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@laxels/docusaurus-plugin-segment": "^1.0.6",
    "@laxels/serve-static": "^1.15.1",
    "@mdx-js/react": "^3.0.1",
    "@types/react": "^18.3.9",
    "classnames": "^2.5.1",
    "clsx": "^2.0.0",
    "docusaurus-lunr-search": "^3.5.0",
    "docusaurus-plugin-image-zoom": "^0.1.4",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "plugin-image-zoom": "^1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-loadable": "^5.5.0",
    "search-insights": "^2.17.2",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.5.2",
    "@jest/globals": "^29.5.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/yargs": "^17.0.23",
    "axios": "^1.3.5",
    "fast-xml-parser": "^4.1.2",
    "immer": "^9.0.19",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "yargs": "^17.7.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "packageManager": "yarn@1.22.22"
}
