{
  "name": "docodile",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build:prod": "docusaurus build --config docusaurus.config.js",
    "build:beta": "docusaurus build --config docusaurus-beta.config.js",
    "swizzle": "docusaurus swizzle",
    "deploy:prod": "yarn build:prod && yarn deploy-gae:prod",
    "deploy:beta": "yarn build:beta && yarn deploy-gae:beta",
    "deploy-gae:prod": "gcloud config set project wandb-docs && gcloud app deploy app.yaml",
    "deploy-gae:beta": "gcloud config set project wandb-docs && gcloud app deploy app-beta.yaml",
    "rebuild-search-index:prod": "docker run -it --env-file=.env.local -e \"CONFIG=$(cat docSearchConfig.json | jq -r tostring)\" algolia/docsearch-scraper",
    "rebuild-search-index:beta": "docker run -it --env-file=.env.local -e \"CONFIG=$(cat docSearchConfig-beta.json | jq -r tostring)\" algolia/docsearch-scraper",
    "clear": "docusaurus clear",
    "serve": "node server.js",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "test": "jest",
    "redirect-404s": "ts-node scripts/getRedirectsFrom404s"
  },
  "dependencies": {
    "@docusaurus/core": "^2.3.1",
    "@docusaurus/plugin-google-gtag": "^2.3.1",
    "@docusaurus/plugin-google-tag-manager": "^2.3.1",
    "@docusaurus/preset-classic": "^2.3.1",
    "@laxels/docusaurus-plugin-segment": "^1.0.6",
    "@laxels/serve-static": "^1.15.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "docusaurus-lunr-search": "^2.3.2",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "plugin-image-zoom": "^1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.3.1",
    "@google-cloud/bigquery": "^6.1.0",
    "@jest/globals": "^29.5.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@tsconfig/node-lts": "^18.12.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/yargs": "^17.0.23",
    "axios": "^1.3.5",
    "fast-xml-parser": "^4.1.2",
    "immer": "^9.0.19",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yargs": "^17.7.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
